				| #{cards.day}			.card__sale				span #{cards.sale +'%'}			.card__icon				img(src="#{cards.image}", alt="icon")			.card__category				| #{cards.category}			a(href="#{cards.link}", target="_blank", class="card__link", onclick="pushStateToDataLayerDay(event) ('CalendarDay', 'day')")				| #{cards.titleEmpty}//- НЕ Активнаяmixin disable-cards(cards)	.card__wrapper		div(class="card-disable card-default" id="cards_"+`${cards.id}` data=`${cards.data}`)			.card-top				img(src=`${cards.image}`, alt="image", width="270px", height="184px", class="card-top__img")				.card-top__day					|#{cards.id + ' августа'}			.card-middle				.card-middle__title					|!{cards.category}				.card-middle__sale					|#{cards.sale + '%'}				.card-middle__promocode					|Промокод: 					span #{cards.promocode}			.card-bottom				a(href=`${cards.link}`, target="_blank", class="card-bottom__link", onclick="pushStateToDataLayerDay(event) ('CalendarDay', 'day')")					| #{cards.titleEmpty}//- Активнаяmixin active-cards(cards)	.card-wrapper		div(class="card-active card-default" id="cards_"+`${cards.id}` data=`${cards.data}`)			.card-top				img(src=`${cards.image}`, alt="image", width="270px", height="184px", class="card-top__img")				.card-top__day					|#{cards.id + ' августа'}			.card-middle				.card-middle__title					|!{cards.category}				.card-middle__sale					|#{cards.sale}					span %				.card-middle__promocode					|Промокод: 					span #{cards.promocode}			.card-bottom				a(href=`${cards.link}`, target="_blank", class="card-bottom__link", onclick="pushStateToDataLayerDay(event) ('CalendarDay', 'day')")					| #{cards.goPurchase}//- Активная в хедереmixin active-cards-header(cards)	.card-wrapper		div(class="card-active card-default" id="cards-item-"+`${cards.id}` datahead=`${cards.data}`)			.card-sale-day				| Скидка дня			.card-top				img(src=`${cards.image}`, alt="image", width="270px", height="184px", class="card-top__img")				.card-top__day					|#{cards.id + ' августа'}			.card-middle				.card-middle__title					|!{cards.category}				.card-middle__sale					|#{cards.sale}					span %				.card-middle__promocode					|Промокод: 					span #{cards.promocode}			.card-bottom				a(href=`${cards.link}`, target="_blank", class="card-bottom__link", onclick="pushStateToDataLayerDay(event) ('CalendarDay', 'day')")					| #{cards.goPurchase}//- //- Пустая - не активная карточка//- mixin disable-cards-empty(cards)//- 	.card__wrapper//- 		.card__disable.card__default//- 			.card__number//- 				| #{cards.id}//- 			.card__day//- 				| #{cards.day}//- 			.card__icon//- 				img(src="#{cards.image}", alt="icon")//- 			.card__category//- 				| #{cards.titleEmpty}//-Активная карточка//- mixin active-cards(cards)//- 	.card__wrapper//- 		div(class="card__active card__default" id="cards_"+"#{cards.id}" data="#{cards.data}")//- 			.card__number//- 				| #{cards.id}//- 			.card__day//- 				| #{cards.day}//- 			.card__sale//- 				span #{cards.sale + '%'}//- 			.card__promocode//- 				| По промокоду: //- 				span #{cards.promocode}//- 			.card__icon//- 				img(src="#{cards.image}", alt="icon")//- 			.card__category//- 				| #{cards.category}//- 			a(href="#{cards.link}", target="_blank", class="card__link", onclick="pushStateToDataLayerDay(event) ('CalendarDay', 'day')")//- 				| #{cards.goPurchase + '>'}				//- a(href="#", target="_blank", class="card-bottom__link") 					span Перейти к покупке//- завершеноmixin completed-cards(cards)	.card__wrapper		.card__completed.card__default			.completed				| #{cards.titleCompleted}			.card__number				| #{cards.id}			.card__day				| #{cards.day}			.card__sale				span #{cards.sale + '%'}			.card__icon				img(src="#{cards.image}", alt="icon")			.card__category				| #{cards.category}//- пустая карточкаmixin empty-cards(cards)	.card__wrapper		div(class="card__empty card__default" id="cards_"+"#{cards.id}" data="#{cards.data}")			.card__number				| #{cards.id}			.card__day				| #{cards.day}			.card__icon				img(src="#{cards.image}", alt="icon")			.card__category				| #{cards.titleEmpty}//- завершено пустая карточкаmixin completed-cards-empty(cards)	.card__wrapper		div(class="completed-cards-empty card__default" id="cards_"+"#{cards.id}" data="#{cards.data}")			.completed				| #{cards.titleCompleted}			.card__number				| #{cards.id}			.card__day				| #{cards.day}			.card__icon				img(src="#{cards.image}", alt="icon")			.card__category				| #{cards.titleEmpty}//- Товарыmixin goods(data)	.goods__item		.goods__wrap__img			a(class="goods__picture__wrap" href= "#{data.link}" target="_blank")				img(class="goods__picture" src="#{pathCardImgAdmin + data.id + '.jpg'}")		.goods__wrap__name			.goods__name				| #{data.title}		.goods__wrap__price			.price__block				.goods__retail__price					| #{data.oldPrice}.-				.goods__start__price					| #{data.price}.-		a(class="goods__link", href= "#{data.link}") Купить		//- Товары с промокодом и выгодойmixin goods-promocode(data)	.goods__item.goods__item__promocode		.goods__sale			| Выгода 			br			span #{data.sale}		.goods__wrap__img			a(class="goods__picture__wrap" href= "#{data.link}" target="_blank")				img(class="goods__picture" src="#{pathCardImgAdmin + data.id + '.jpg'}")		.goods__wrap__name			.goods__name				| #{data.title}		.promocode			| Промокод: 			span #{data.promocode}		.goods__wrap__price			.price__block				.goods__retail__price					| #{data.oldPrice}.-				.goods__start__price					| #{data.price}.-		a(class="goods__link", href= "#{data.link}") Купить				//- Товары для аукционаmixin goods-auction(data)	.goods__item.goods__item__auction		.goods__wrap__img			a(class="goods__picture__wrap" href= "#{data.link}" target="_blank")				img(class="goods__picture" src="#{pathCardImgAdmin + data.id + '.jpg'}")		.goods__wrap__name			.goods__name				| #{data.title}		.goods__wrap__price			.price__block				.goods__retail 					| Розничная цена				.goods__price__retail					| #{data.oldPrice}.-			.price__block				.goods__start					| Стартовая цена				.goods__price__start					| #{data.price}.-		a(class="goods__link", href= "#{data.link}") О товаре			//- Сетка по 3 товараmixin grid-4()	.col-lg-4.col-md-4.col-sm-6.col-xs-12		block			//- Сетка по 4 товараmixin grid-3()	.col-lg-3.col-md-4.col-sm-7.col-xs-15		block			//- Футер служебкаmixin footer-text()	footer.footer		.container			.row				.col-lg-15.col-md-15.col-sm-15.col-xs-15					.footer__block						.footer__text							|Предложение действительно для зарегистрированных покупателей Юлмарта — физических лиц. Количество товара ограничено. Приведенное изображение может отличаться от внешнего вида товара в центрах исполнения заказов. Существенная информация — у консультантов в городских центрах исполнения заказов Юлмарт и на сайте 							a(href="https://www.ulmart.ru/", target="_blank") ulmart.ru.							br							|Категория сайта 18+. Непубличное акционерное общество «Юлмарт», 197227, Санкт-Петербург, пр. Сизова, д. 2, лит. А, ОГРН 1089848006423; ООО «Юлмарт РСК», 197227, Санкт-Петербург, пр. Сизова, д. 2, лит. А, ОГРН 1117847607900.